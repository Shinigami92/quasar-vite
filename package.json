{
  "name": "quasar-vite",
  "version": "0.0.0",
  "description": "Frontend",
  "author": "Christopher Quadflieg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist pnpm-lock.yaml node_modules",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:cov:watch": "vitest --coverage",
    "cypress": "cypress",
    "cypress:open": "run-p --race start \"cypress open\"",
    "cypress:run": "run-p --race start \"cypress run\"",
    "test:e2e": "pnpm run cypress:run",
    "cypress:open:ct": "cypress open --component",
    "cypress:run:ct": "cypress run --component",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.vue src"
  },
  "dependencies": {
    "@quasar/extras": "~1.15.1",
    "@vueuse/core": "~9.1.0",
    "@vueuse/shared": "~9.1.0",
    "vue": "~3.2.37",
    "vue-i18n": "~9.2.2",
    "vue-router": "~4.1.3"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "~3.0.0",
    "@cypress/vue": "~4.0.0",
    "@originjs/vite-plugin-content": "~1.0.1",
    "@prettier/plugin-pug": "~2.2.0",
    "@quasar/vite-plugin": "~1.1.1",
    "@types/node": "~18.6.4",
    "@types/web-bluetooth": "~0.0.15",
    "@typescript-eslint/eslint-plugin": "~5.32.0",
    "@typescript-eslint/parser": "~5.32.0",
    "@vitejs/plugin-vue": "~3.0.1",
    "@vue/compiler-sfc": "~3.2.37",
    "@vue/test-utils": "~2.0.2",
    "c8": "~7.12.0",
    "cypress": "~10.4.0",
    "eslint": "~8.21.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-define-config": "~1.6.0",
    "eslint-gitignore": "~0.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-vue": "~9.3.0",
    "eslint-plugin-vue-pug": "~0.5.4",
    "happy-dom": "~6.0.4",
    "jsdom": "~20.0.0",
    "npm-run-all": "~4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.0.2",
    "pug": "~3.0.2",
    "quasar": "~2.7.7",
    "react": "~18.2.0",
    "rimraf": "~3.0.2",
    "sass": "~1.32.13",
    "ts-essentials": "~9.2.0",
    "typescript": "~4.7.4",
    "vite": "~3.0.4",
    "vitest": "~0.21.0",
    "vue-eslint-parser": "~9.0.3",
    "vue-tsc": "~0.39.5"
  }
}
