{
  "name": "quasar-vite",
  "version": "0.0.0",
  "description": "Frontend",
  "author": "Christopher Quadflieg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist pnpm-lock.yaml node_modules",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "vite",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "cypress": "cypress",
    "cypress:open": "run-p --race start 'cypress open'",
    "cypress:open:ct": "cypress open-ct",
    "cypress:run": "run-p --race start 'cypress run'",
    "cypress:run:ct": "cypress run-ct",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.vue src"
  },
  "dependencies": {
    "@quasar/extras": "~1.12.4",
    "@vueuse/core": "~7.5.3",
    "vue": "~3.2.27",
    "vue-i18n": "~9.2.0-beta.28",
    "vue-router": "~4.0.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "~2.2.2",
    "@cypress/vue": "3.0.0-beta.4",
    "@originjs/vite-plugin-content": "~1.0.1",
    "@prettier/plugin-pug": "~1.19.1",
    "@quasar/vite-plugin": "~1.0.4",
    "@types/node": "~17.0.10",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "@vitejs/plugin-vue": "~2.0.1",
    "@vue/compiler-sfc": "~3.2.27",
    "@vue/test-utils": "~2.0.0-rc.18",
    "c8": "~7.11.0",
    "cypress": "~9.3.1",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-define-config": "~1.2.2",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-vue": "~8.3.0",
    "eslint-plugin-vue-pug-sfc": "~1.0.0-alpha.20",
    "happy-dom": "~2.27.0",
    "npm-run-all": "~4.1.5",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "~2.3.4",
    "pug": "~3.0.2",
    "quasar": "~2.4.11",
    "rimraf": "~3.0.2",
    "sass": "1.32.0",
    "typescript": "~4.5.4",
    "vite": "~2.7.13",
    "vitest": "~0.1.23",
    "vue-tsc": "~0.30.6"
  }
}
